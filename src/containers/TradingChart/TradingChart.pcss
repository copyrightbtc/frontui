.group-wWM3zP_M- {
  background-color: transparent !important;
}

.trading-chart {
  background: transparent;
  height: 100%;
  left: 0;
  position: relative;
  top: 0;
  width: 100%;

  #sfortrade_chart {
    background: transparent;
    height: calc(100% - 39px);
  }
}

.trading-chart--error {
  align-items: center;
  background: rgba(255, 255, 255, 0.1);
  border: 1px solid white;
  color: var(--system-red);
  display: flex;
  font-size: 16px;
  justify-content: center;
}

